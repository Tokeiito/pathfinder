{
  "name": "goryn-clade/pathfinder",
  "description": "Mapping tool for EVE ONLINE",
  "type": "project",
  "homepage": "https://github.com/goryn-clade/pathfinder",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Friedrich",
      "email": "pathfinder@exodus4d.de"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "lock": true
  },
  "autoload": {
    "psr-4": {
      "Exodus4D\\Pathfinder\\": "app/"
    }
  },
  "readme": "README.md",
  "require": {
    "php-64bit": ">=7.4",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-ctype": "*",
    "ext-gd": "*",
    "bcosca/fatfree-core": "3.8.*",
    "ikkez/f3-cortex": "1.7.*",
    "ikkez/f3-sheet": "0.4.*",
    "xfra35/f3-cron": "1.2.*",
    "monolog/monolog": "2.6.0",
    "swiftmailer/swiftmailer": "6.2.*",
    "league/html-to-markdown": "5.1.*",
    "cache/redis-adapter": "1.1.*",
    "cache/filesystem-adapter": "1.2.*",
    "cache/array-adapter": "1.2.*",
    "cache/void-adapter": "1.2.*",
    "cache/namespaced-cache": "1.2.*",
    "react/socket": "1.12.*",
    "react/promise-stream": "1.5.*",
    "clue/ndjson-react": "1.3.*",
    "firebase/php-jwt": "^6.3.2",
    "tyrheimdaleve/pathfinder_esi": "2.3.*"
  },
  "repositories": {
    "pathfinder_esi": {
      "type": "vcs",
      "url": "https://github.com/TyrHeimdalEVE/pathfinder_esi"
    }
  },
  "suggest": {
    "ext-redis": "Redis can be used as cache backend."
  }
}
